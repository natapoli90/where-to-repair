  <div class="container center">
    <br  />
    <% if current_admin %>
    <%= link_to "New Service", new_service_path, class: "btn waves-effect waves-light lime darken-1" %>
    <% end %>
  </div>
  <br  />

  <div class="container">
    <div class="row">
      <div class="col s12">
        <div class="card">
          <div class="card-content">
            <% @services.order('category DESC,name ASC').each do |s| %>
              <p class="card-title"><%= s.category %>: <%= s.name %></p>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
